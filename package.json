{
  "name": "vite-rollup-plugins",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Matias Capeletto",
  "description": "A playground for Vite compatible official rollup plugins",
  "packageManager": "pnpm@7.9.0",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "copy": "shx mkdir -p public/shiki && shx cp -r node_modules/shiki/{dist,languages,samples,themes} public/shiki/",
    "prepare": "pnpm run copy",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open"
  },
  "dependencies": {
    "@antfu/utils": "^0.5.2",
    "@vueuse/core": "^9.1.0",
    "fuse.js": "^6.6.2",
    "htm": "^3.1.1",
    "nanoid": "^3.3.4",
    "shiki": "^0.11.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@iconify/json": "^1.1.461",
    "@rollup/plugin-auto-install": "^2.2.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-dsv": "^2.0.3",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-graphql": "^1.1.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-inject": "^4.0.4",
    "@rollup/plugin-legacy": "^2.2.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/plugin-virtual": "^2.1.0",
    "@rollup/plugin-yaml": "^3.1.0",
    "@types/node": "^14.18.24",
    "@vitejs/plugin-vue": "^3.0.3",
    "babel-plugin-htm": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "graphql": "^15.8.0",
    "pnpm": "^7.9.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-visualizer": "^4.2.2",
    "shx": "^0.3.4",
    "typescript": "^4.7.4",
    "vite": "3.0.8",
    "vite-plugin-components": "^0.13.3",
    "vite-plugin-icons": "^0.6.5"
